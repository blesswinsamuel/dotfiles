URxvt.font: xft:DejaVu Sans Mono:size=8

! Theme

URxvt.depth: 32
*background: [95]#1C1C1C
*foreground: #DEDEDE

! black
*color0  : #2E3436
*color8  : #555753
! red
*color1  : #CC0000
*color9  : #EF2929
! green
*color2  : #4E9A06
*color10 : #8AE234
! yellow
*color3  : #C4A000
*color11 : #FCE94F
! blue
*color4  : #3465A4
*color12 : #729FCF
! magenta
*color5  : #75507B
*color13 : #AD7FA8
! cyan
*color6  : #06989A
*color14 : #34E2E2
! white
*color7  : #D3D7CF
*color15 : #EEEEEC

!
! scrollbar
!
URxvt*scrollBar:          true
URxvt*thickness:          4
URxvt*scrollColor:        #CCCCCC
URxvt*troughColor:        #FF0000
URxvt*scrollBar_floating: true
URxvt*scrollBar_right:    true
URxvt*scrollstyle:        plain
